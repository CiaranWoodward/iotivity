# makefile for iotlite zephyr connectivity.
#

TOP=../../..
EXTOUT=$(TOP)/extout
CSDK=$(TOP)/resource/csdk

CC=gcc
MAKEDEPEND=gccmakedep	# makedepend doesn't seem to work here
CFLAGS= -Wall -std=c99
INCLUDES = -I$(CSDK)/stack/include \
	   -I$(CSDK)/stack/include/internal \
	   -I$(CSDK)/connectivity/inc
CFILES= zephyr.c \
	ocrandom.c \
	oic_malloc.c
COBJS= $(CFILES:.c=.o)
OUT= $(EXTOUT)/zephyr/libzephyr.a

.SUFFIXES: .c

all:	$(OUT)

.c.o:
	$(CC) $(CFLAGS) $(CFG) $(INCLUDES) -c $< -o $@

$(OUT): $(COBJS)
	ar rcs $(OUT) $(COBJS)

depend:
	$(MAKEDEPEND) -- $(CFLAGS) -- $(CFG) $(INCLUDES) $(CFILES)

clean:
	rm -f $(COBJS) $(OUT) Makefile.bak

# DO NOT DELETE
